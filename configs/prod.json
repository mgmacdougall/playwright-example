{
    "baseURL": "https://playwright.dev/",
    "timeout": 30000,
    "headless": true,
    "viewport": {
        "width": 1280,
        "height": 720
    },
    "screenshot": "only-on-failure",
    "video": "retain-on-failure",
    "reporter": [
        [
            "html",
            {
                "outputFolder": "test-report",
                "open": "never"
            }
        ]
    ]
}